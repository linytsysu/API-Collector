{
    "manifest_version": 2,
    
    "name": "API Collector",
    "description": "Add API links to API Collector",
    "version": "1.0",
    
    "browser_action": {
        "default_icon": {
            "19": "icons/icon19.png",
            "38": "icons/icon38.png"
        },
        "default_title": "API Collector",
        "default_popup": "html/popup.html"
    },
    
    "permissions": [
        "activeTab",
        "bookmarks",
        "storage"
    ]

}
